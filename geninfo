#! /bin/bash

echo 

if test -z "$1"
then 
	echo "No command-line arguments"
else
	echo "First command line argument is $1"
fi

echo

if /bin/test -z "$1"
then
	echo "No command-line arguments"
else
	echo "First command line argument is $1"
fi

echo

if [ -z "$1"]
then
	echo "No command-line arguments"
else
	echo "First command line argument is $1"
fi

echo

if /bin/[ -z "$1"]
then
	echo "No command-line arguments"
else
	echo "First command line argument is $1"
fi

echo 
